{
	"name": "Amazon Store Switcher",
	"version": "1.0",
	"description": "Switch between different Amazon international stores.",
	"browser_action": {
		"default_icon": "icon.png",
		"popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"http://www.amazon.ca/*",
		"http://www.amazon.cn/*",
		"http://www.amazon.co.jp/*",
		"http://www.amazon.co.uk/*",
		"http://www.amazon.com/*",
		"http://www.amazon.de/*",
		"http://www.amazon.es/*",
		"http://www.amazon.fr/*"
	]
}
